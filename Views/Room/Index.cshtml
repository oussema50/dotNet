
@model List<projetDotNet.Models.Room>

<div class="room_header">
    <h1>Rooms</h1>
    <button class="btn btn-primary">Add Room</button>
</div>

<div class="row align-items-start">
    @foreach (var item in Model)
    {
        <div class="col">
            <div class="card" style="width: 18rem;">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
                <div class="card-body">
                    <h5>Room Number: @item.roomName</h5>
                    <h5>Room Size: @item.roomSize</h5>
                    @if (@item.isAvailable)
                    {
                        <small style="background-color: #04ad04; color: white; padding: 2px 4px;">isAvailable</small>
                    }
                    else
                    {
                        <small style="background-color: #e92626; color: white; padding: 2px 4px;">isAvailable</small>
                    }
                    <div class="link" style="margin-top:10px;">
                        <a href="/room/getRoom/@item.roomID" class="btn btn-primary">room details</a>
                    </div>
                </div>
            </div>
        </div>
        

    }

</div>


